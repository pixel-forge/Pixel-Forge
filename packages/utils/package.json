{
  "name": "@pixelforge/utils",
  "version": "0.0.0",
  "private": false,
  "description": "Shared TypeScript utilities for Pixel Forge libraries.",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./array": {
      "types": "./dist/array/index.d.ts",
      "import": "./dist/array/index.mjs",
      "require": "./dist/array/index.cjs"
    },
    "./object": {
      "types": "./dist/object/index.d.ts",
      "import": "./dist/object/index.mjs",
      "require": "./dist/object/index.cjs"
    },
    "./timing": {
      "types": "./dist/timing/index.d.ts",
      "import": "./dist/timing/index.mjs",
      "require": "./dist/timing/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "tsup && tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  }
}
